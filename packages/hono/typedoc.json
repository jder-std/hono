{
    "$schema": "https://typedoc-plugin-markdown.org/schema.json",
    "entryPoints": [
        "./src/response/index.ts",
        "./src/middlewares/body-limit.ts",
        "./src/middlewares/ip-limit.ts",
        "./src/middlewares/time-limit.ts"
    ],
    "includeVersion": true,
    "out": "./../../apis/hono",
    "cleanOutputDir": true,
    "plugin": [
        "typedoc-plugin-markdown",
        "./../../scripts/insert-link.js"
    ],
    "readme": "none",
    "entryFileName": "README",
    "hidePageHeader": true,
    "useCodeBlocks": true,
    "indexFormat": "table"
}
